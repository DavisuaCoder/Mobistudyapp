<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mobistudy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mobistudy App</name>
    <description>Mobistudy Patients' App</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-nativestorage" spec="^2.3.2" />
    <engine name="android" spec="^7.1.1" />
    <plugin name="cordova-plugin-health" spec="^1.1.0">
        <variable name="HEALTH_READ_PERMISSION" value="MobistudyApp needs read access" />
        <variable name="HEALTH_WRITE_PERMISSION" value="MobistudyApp needs write access" />
    </plugin>
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.2" />
</widget>
