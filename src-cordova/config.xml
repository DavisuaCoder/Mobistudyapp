<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mobistudy.app" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mobistudy App</name>
    <description>Mobistudy Participants' App</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-nativestorage" spec="^2.3.2" />
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="^0.8.5" />
    <plugin name="cordova-android-support-gradle-release" spec="^2.0.1">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-health" spec="^1.1.1">
        <variable name="HEALTH_READ_PERMISSION" value="Mobistudy needs read access" />
        <variable name="HEALTH_WRITE_PERMISSION" value="Mobistudy needs write access" />
    </plugin>
    <preference name="DisallowOverscroll" value="true" />
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="^4.5.4" />
</widget>
